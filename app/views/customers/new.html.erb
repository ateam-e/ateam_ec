<h1>Please wright down your information</h1>
<%= form_for(@customer) do |c| %>
  <div class="">
    name
    <%= c.text_field :name %>
  </div>
  <div class="">
    email
    <%= c.email_field :email %>
  </div>
  <div class="">
    confirm-email
    <%= c.email_field :email %>
  </div>
  <div class="">
    password
    <%= c.password_field :password %>
  </div>
  <div class="">
    gender
    <%= c.radio_button :gender, 'male', :checked => true  %>Mail <%= c.radio_button :gender, 'female' %>Femail
  </div>
  <div class="">
    date of birth
    <%= c.text_field :dateofbirth %>

  </div>
  <div class="">
    image
    <%= c.file_field :image %>
  </div>


  <%= c.submit "send" %>
  <input type="button" name="" value="facebook">
<% end %>
