<h1>Please wright down your informations</h1>
<%= form_for(@customer) do |c| %>
  <div class="">
    名前
    <%= c.text_field :name %>
  </div>
  <div class="">
    メールアドレス
    <%= c.email_field :email %>
  </div>
  <div class="">
    メールアドレス（確認用）
    <%= c.email_field :email %>
  </div>
  <div class="">
    パスワード
    <%= c.password_field :password %>
  </div>
  <div class="">
    性別
    <%= c.radio_button :gender, 'male', :checked => true  %>Mail <%= c.radio_button :gender, 'female' %>Femail
  </div>
  <div class="">
    生年月日
    <%= c.text_field :dateofbirth %>

  </div>
  <div class="">
    住所
    <%= c.text_field :address %>

  </div>
  <div class="">
    登録画像
    <%= c.file_field :image %>
  </div>


  <%= c.submit "送信" %>
  <input type="button" name="" value="facebook">
<% end %>
