<p id="notice"><%= notice %></p>

<h1>Finalorders</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Phonenumber</th>
      <th>Address</th>
      <th>Email</th>
      <th>Delivery</th>
      <th>Payment</th>
      <th>Gift</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @finalorders.each do |finalorder| %>
      <tr>
        <td><%= finalorder.username %></td>
        <td><%= finalorder.phonenumber %></td>
        <td><%= finalorder.address %></td>
        <td><%= finalorder.email %></td>
        <td><%= finalorder.delivery %></td>
        <td><%= finalorder.payment %></td>
        <td><%= finalorder.gift %></td>
        <td><%= link_to 'Show', finalorder %></td>
        <td><%= link_to 'Edit', edit_finalorder_path(finalorder) %></td>
        <td><%= link_to 'Destroy', finalorder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Finalorder', new_finalorder_path %>
